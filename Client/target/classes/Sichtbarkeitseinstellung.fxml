<AnchorPane prefHeight="313.0" prefWidth="507.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.client.Soziales.SichtbarkeitKontoController">
   <children>
      <Text fill="#ff9d00" layoutX="105.0" layoutY="50.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Profil - Sichtbarkeitseinstellung">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Text>
      <Button fx:id="backButton" layoutX="413.0" layoutY="272.0" mnemonicParsing="false" onAction="#backWindow" prefHeight="25.0" prefWidth="73.0" text="Zurück">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Text fill="#ff4e00" layoutX="37.0" layoutY="93.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Lege fest, wer deine Daten sehen darf:">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Text>
      <RadioButton fx:id="meButton" layoutX="83.0" layoutY="116.0" mnemonicParsing="false" onAction="#setJustMe" text="Nur Ich">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></RadioButton>
      <RadioButton fx:id="friendButton" layoutX="83.0" layoutY="148.0" mnemonicParsing="false" onAction="#setFriends" text="Meine Freunde">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></RadioButton>
      <RadioButton fx:id="allButton" layoutX="83.0" layoutY="183.0" mnemonicParsing="false" onAction="#setAll" prefHeight="17.0" prefWidth="182.0" text="ALLE (keine Einschränkung)">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></RadioButton>
      <Button fx:id="saveButton" layoutX="327.0" layoutY="272.0" mnemonicParsing="false" onAction="#saveSettings" prefHeight="25.0" prefWidth="73.0" text="Speichern">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Text fx:id="invalidSave" fill="RED" layoutX="124.0" layoutY="289.0" strokeType="OUTSIDE" strokeWidth="0.0" textAlignment="CENTER" wrappingWidth="194.4000076293944">
         <font>
            <Font size="14.0" />
         </font>
      </Text>
      <ImageView fitHeight="57.0" fitWidth="94.0" layoutX="37.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Image/privacy.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
